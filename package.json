{
  "name": "art-of-quality-library",
  "version": "1.0.0",
  "description": "A curated collection of ideas worth revisiting",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "live-server public --port=8080",
    "dev:watch": "nodemon --watch src --watch content -e js,css,md,html --exec npm run build",
    "build": "node scripts/build.js",
    "build:prod": "NODE_ENV=production npm run build",
    "test": "jest",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:e2e": "playwright test",
    "lint": "eslint src scripts tests",
    "format": "prettier --write '**/*.{js,css,html,md}'",
    "new": "node scripts/new-content.js",
    "generate-index": "node scripts/generate-index.js",
    "serve": "live-server public",
    "deploy": "npm run build:prod && npm run deploy:github",
    "deploy:github": "gh-pages -d public"
  },
  "keywords": ["knowledge-management", "digital-garden", "zettelkasten"],
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://YOUR_USERNAME.github.io/art-of-quality-library",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "gh-pages": "^6.1.0"
  },
  "dependencies": {
    "clean-css": "^5.3.3",
    "date-fns": "^3.0.6",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "html-minifier-terser": "^7.2.0",
    "inquirer": "^9.2.12",
    "marked": "^11.1.0",
    "minisearch": "^6.3.0",
    "rss": "^1.2.2",
    "slugify": "^1.6.6",
    "terser": "^5.26.0"
  }
}
